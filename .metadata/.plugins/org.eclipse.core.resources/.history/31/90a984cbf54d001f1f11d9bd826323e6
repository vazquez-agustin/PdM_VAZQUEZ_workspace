#ifndef API_DISPLAYLCD_INC_API_DISPLAYLCD_H_
#define API_DISPLAYLCD_INC_API_DISPLAYLCD_H_


#include <stdint.h>

// Initialize the LCD
void LCD_Init(void);

// Clear the LCD display
void LCD_Clear(void);

// Set the cursor position
void LCD_SetCursor(uint8_t row, uint8_t col);

// Print a string on the LCD
void LCD_Print(const char *str);

#endif /* API_DISPLAYLCD_INC_API_DISPLAYLCD_H_ */
